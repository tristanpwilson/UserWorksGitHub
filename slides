  <script type="text/javascript">  /* Highlight for page scroll on employee anchor link click */

   $(document).ready(boxSizer);
   
   $(window).resize(boxSizer);
   
   
   function boxSizer(){
     var frw = $('.fr').width();
     $('.fr').css({'min-height':frw / 1.58 + 'px'});
   }


